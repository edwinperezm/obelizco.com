  "scripts": {
    "preinstall": "npx only-allow npm",
    "postinstall": "if [ -z \"$NODE_ENV\" ] || [ \"$NODE_ENV\" != \"production\" ]; then npm run check; fi",
    "dev:frontend": "vite --port 3001",
    "dev:backend": "netlify dev",
    "dev": "netlify dev",
    "build": "vite build && npm run build:functions",
    "prebuild:functions": "mkdir -p netlify/functions/dist/src",
    "build:functions": "rm -rf netlify/functions/dist/* && tsc -p tsconfig.functions.json --outDir netlify/functions/dist && cp -r netlify/functions/src/* netlify/functions/dist/ && cp netlify/functions/package.json netlify/functions/dist/ && cd netlify/functions/dist && npm install --production && cd ../../..",
    "preview": "vite preview",
    "start": "netlify dev",
    "check": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "netlify:dev": "netlify dev"
  },
